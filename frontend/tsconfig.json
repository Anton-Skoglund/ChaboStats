{
  "extends": "@tsconfig/svelte/tsconfig.json",
  "compilerOptions": {
    "composite": true,
    "target": "ES2022",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "@frontendTypes/*": [
        "src/frontendTypes/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@lib/*": [
        "src/lib/*"
      ],
      "@config/*": [
        "src/config/*"
      ],
      "@shared/*": [
        "../shared/*"
      ]
    },
    "allowJs": true,
    "checkJs": true,
    "isolatedModules": true,
    "moduleDetection": "force",
    "outDir": "dist"
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.js",
    "src/**/*.svelte",
    "../shared/**/*.ts"
, "src/lib/iconConverter.py"  ],
  "references": [
    {
      "path": "../shared"
    }
  ]
}